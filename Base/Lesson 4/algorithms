//Task 5
function fib(n){
    var arrFib = [];
    var a = arrFib[0] = 1;
    var b = arrFib[1] = 1;
    for (i = 2; i <= n; i++) {
        var c = a + b;
        a = b;
        b = c;
        arrFib.push(c);
    };
    return arrFib[n-1];
};

//Task 6
function strLength(str, length){
        if (str.length >= length) {
        var result = str.slice(0, length)+"...";
    } else {
        result = str;
    };
    return result;
};

